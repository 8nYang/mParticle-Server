{
  "name": "mP-Media",
  "version": "1.0.0",
  "description": "A JavaScript project",
  "main": "./dist/index.js",
  "targets": {
    "frontend": {
      "source": "./src/index.js"
    },
    "backend": {
      "source": "./src/public/demo.js"
    }
  },
  "scripts": {
    "dev:watch-backend": "node ./node_modules/parcel-bundler/bin/cli watch ./src/index.js ./src/public/mp/*.js --target node --out-dir ./dist/",
    "dev:watch-frontend": "node ./node_modules/parcel-bundler/bin/cli watch ./src/public/*.html ./src/public/img/* ./src/public/*.demo.js ./src/public/*service-worker.js ./src/public/*-index.js --out-dir ./dist/public/",
    "dev:hot-reload": "node ./node_modules/nodemon/bin/nodemon --watch ./dist/ ./dist/index.js",
    "start": "node ./node_modules/npm-run-all/bin/npm-run-all -p -r dev:watch-backend dev:watch-frontend dev:hot-reload",
    "build": "node ./node_modules/parcel-bundler/bin/cli build ./src/index.js --target node --out-dir ./dist/",
    "lint": "node ./node_modules/eslint/bin/eslint . --ext .js --fix"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^6.2.0",
    "@mparticle/web-appboy-kit": "^2.0.11",
    "@mparticle/web-google-analytics-kit": "^2.1.12",
    "@mparticle/web-intercom-kit": "^2.0.1",
    "@mparticle/web-media-sdk": "^1.4.7",
    "@mparticle/web-sdk": "^2.23.2",
    "body-parser": "^1.20.2",
    "child_process": "^1.0.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parse": "^5.4.0",
    "express": "^4.18.2",
    "express-generator": "^4.16.1",
    "https": "^1.0.0",
    "morgan": "^1.10.0",
    "mparticle": "^1.0.8",
    "multer": "^1.4.5-lts.1",
    "parcel": "^2.10.0"
  }
}
