{
  "providers": [
    "awscloudformation"
  ],
  "projectName": "mParticleProfileServ",
  "version": "3.1",
  "frontend": "javascript",
  "javascript": {
    "framework": "none",
    "config": {
      "SourceDir": "src",
      "DistributionDir": "dist",
      "BuildCommand": "node ./node_modules/parcel-bundler/bin/cli build ./src/index.js --target node --out-dir ./dist/",
